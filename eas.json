{
  "cli": {
    "version": ">= 9.0.5"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "enterpriseProvisioning": "adhoc",
        "image": "latest"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "GOOGLE_SERVICES_JSON" : "./google-services.json",
        "GOOGLE_SERVICES_PLIST" : "./GoogleService-Info.plist"
      }
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
